//this for loop allows the counter to start at 70 and end at 110 by increasing by +1 every increment

for (let counter = 70; counter < 111; counter++) {
  console.log(counter)
}


//this for prints 'I would love to visit ' + vacationSpots[i] through Arrays in a for Loop

const vacationSpots = ['Bali', 'Paris', 'Tulum'];

for (let i = 0; i < vacationSpots.length; i++) {
  console.log('I would love to visit ' + vacationSpots[i]);
}
